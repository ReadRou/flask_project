server {
    listen       80;
    server_name  zh_flask;
    access_log  /root/zh_flask/log/access.log;
    error_log  /root/zh_flask/log/error.log;

    location / {
        include        /root/zh_flask/conf/zh_flask.ini;
        uwsgi_pass      0.0.0.0:8080;
        uwsgi_param UWSGI_CHDIR  /root/zh_flask/;
        uwsgi_param UWSGI_SCRIPT manage:run;
    }

}